
# Debian:
#   apt-get install gcc-9-arm-linux-gnueabi
# You need a link 
# 	arm-linux-gnueabi-gcc -> /usr/bin/arm-linux-gnueabi-gcc-9
# in your ~/bin/ or in /usr/local/bin etc.


# https://forum.openwrt.org/t/build-error-gcc-10-x-solved-pls-fix-on-repo/68942
#export CC=gcc-9
make CROSS_COMPILE=arm-linux-gnueabi- ARCH=arm prepare "$@"
make CROSS_COMPILE=arm-linux-gnueabi- ARCH=arm modules_prepare "$@"
make CROSS_COMPILE=arm-linux-gnueabi- ARCH=arm M=drivers/usb/gadget/ modules "$@"

find . -iname "*.ko" -ls
